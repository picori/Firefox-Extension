<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<!DOCTYPE prefwindow SYSTEM "chrome://rebaterobot/locale/preference.dtd">
<prefwindow id="appPreferences"
            xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
            persist="lastSelected screenX screenY"
			title="&pref.title;" 
			buttons="accept,cancel"
			onload="RebateRobot.XUL.Preference.windowOnLoad(event);"
			ondialogaccept="return RebateRobot.XUL.Preference.doSave(event);"
			style="">
  <prefpane id="pane.global" label="&pane.global.title;" src="chrome://rebaterobot/content/pane_global.xul"/>
  <prefpane id="pane.affiliates" label="&pane.affiliates.title;" src="chrome://rebaterobot/content/pane_affiliates.xul"/>
  <script src="chrome://rebaterobot/content/preference.js" />
</prefwindow>